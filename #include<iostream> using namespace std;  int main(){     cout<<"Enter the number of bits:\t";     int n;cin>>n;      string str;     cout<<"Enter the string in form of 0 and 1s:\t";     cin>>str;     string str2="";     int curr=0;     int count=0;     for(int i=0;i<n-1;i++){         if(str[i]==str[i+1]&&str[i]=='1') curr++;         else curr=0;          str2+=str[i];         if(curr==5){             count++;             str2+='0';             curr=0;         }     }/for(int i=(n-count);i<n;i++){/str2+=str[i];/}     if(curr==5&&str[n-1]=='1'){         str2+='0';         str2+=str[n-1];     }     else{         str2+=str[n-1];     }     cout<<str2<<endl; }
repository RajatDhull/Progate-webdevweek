#include<iostream>
using namespace std;

int main(){
    cout<<"Enter the number of bits:\t";
    int n;cin>>n;

    string str;
    cout<<"Enter the string in form of 0 and 1s:\t";
    cin>>str;
    string str2="";
    int curr=0;
    int count=0;
    for(int i=0;i<n-1;i++){
        if(str[i]==str[i+1]&&str[i]=='1') curr++;
        else curr=0;

        str2+=str[i];
        if(curr==5){
            count++;
            str2+='0';
            curr=0;
        }
    }
    // for(int i=(n-count);i<n;i++){
    //     str2+=str[i];
    // }
    if(curr==5&&str[n-1]=='1'){
        str2+='0';
        str2+=str[n-1];
    }
    else{
        str2+=str[n-1];
    }
    cout<<str2<<endl;
}
